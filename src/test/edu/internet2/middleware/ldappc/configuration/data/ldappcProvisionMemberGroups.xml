<?xml version="1.0" encoding="utf-8"?>

<ldappc>

	<grouper>

		<group-queries>

			<attribute-matching-queries>

				<attribute-list>

					<attribute name="attribute1" value="attributeValue1" />

					<attribute name="attribute2" value="attributeValue2" />

				</attribute-list>

			</attribute-matching-queries>

		</group-queries>


		<groups structure="flat" root-dn="ou=root" ldap-object-class="gLdapObjectClass"
			ldap-rdn-attribute="gLdapRdnAttribute" grouper-attribute="id">

			<group-members-dn-list list-attribute="gmdlAttribute" />

		</groups>

	</grouper>


	<source-subject-identifiers>
		<source-subject-identifier source="source1"
			subject-attribute="subjectAttr1">

			<ldap-search base="ldapSearchBase1" scope="object_scope"
				filter="ldapSearchFilter1" />

		</source-subject-identifier>

		<source-subject-identifier source="g:gsa"
			subject-attribute="subjectAttr2">

			<ldap-search base="ldapSearchBase2" scope="subtree_scope"
				filter="ldapSearchFilter2" />

		</source-subject-identifier>

	</source-subject-identifiers>


	<ldap>
		<context>

			<parameter-list>

				<parameter name="initial_context_factory" value="initContextFactory" />

				<parameter name="provider_url" value="providerUrl" />

				<parameter name="security_authentication" value="securityAuthentication" />

				<parameter name="security_principal" value="securityPrincipal" />

				<parameter name="security_credentials" value="securityCredentials" />

			</parameter-list>

		</context>

	</ldap>

</ldappc>
