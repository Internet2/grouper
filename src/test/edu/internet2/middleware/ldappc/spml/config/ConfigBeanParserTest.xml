<?xml version="1.0" encoding="UTF-8"?><ldappc xmlns="http://grouper.internet2.edu/ldappc" xmlns:ldappc="http://grouper.internet2.edu/ldappc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://grouper.internet2.edu/ldappc classpath:/schema/ldappc.xsd">

  <targets id="ActiveDirectory">

    <target id="ad-prod" provider="provider-ad-prod"/>
    <target id="ad-test" provider="provider-ad-test"/>

    

    

  <object id="group" targetId="ad-prod">
      <identifier id="group-dn" targetId="ad-prod"/>
      <attribute name="objectclass"/>
      <attribute name="cn"/>
      <reference id="members-jdbc" name="member" toPSOId="member"/>
    </object>
      <object id="member" targetId="ad-prod">
      <identifier id="member-dn" targetId="ad-prod"/>
    </object>
      <object id="group" targetId="ad-test">
      <identifier id="group-dn" targetId="ad-test"/>
      <attribute name="objectclass"/>
      <attribute name="cn"/>
      <reference id="members-jdbc" name="member" toPSOId="member"/>
    </object>
      <object id="member" targetId="ad-test">
      <identifier id="member-dn" targetId="ad-test"/>
    </object>
   </targets>

  <targets id="OpenLDAP">

    <target id="openldap-prod" provider="provider-openldap-prod"/>
    <target id="openldap-test" provider="provider-openldap-test"/>

    

    

  <object id="group" targetId="openldap-prod">
      <identifier id="group-dn" targetId="openldap-prod"/>
      <attribute name="objectclass"/>
      <attribute name="cn"/>
      <attribute id="hasMember" name="hasMember"/>
      <reference id="members-jdbc" name="member" toPSOId="member"/>
    </object>
      <object id="member" targetId="openldap-prod">
      <identifier id="member-dn" targetId="openldap-prod"/>
      <attribute id="member-isMemberOf" name="isMemberOf"/>
    </object>
      <object id="group" targetId="openldap-test">
      <identifier id="group-dn" targetId="openldap-test"/>
      <attribute name="objectclass"/>
      <attribute name="cn"/>
      <attribute id="hasMember" name="hasMember"/>
      <reference id="members-jdbc" name="member" toPSOId="member"/>
    </object>
      <object id="member" targetId="openldap-test">
      <identifier id="member-dn" targetId="openldap-test"/>
      <attribute id="member-isMemberOf" name="isMemberOf"/>
    </object>
   </targets>

</ldappc>