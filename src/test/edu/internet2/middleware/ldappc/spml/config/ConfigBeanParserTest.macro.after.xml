<?xml version="1.0" encoding="utf-8"?>

<ldappc xmlns="http://grouper.internet2.edu/ldappc"
        xmlns:ldappc="http://grouper.internet2.edu/ldappc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://grouper.internet2.edu/ldappc classpath:/schema/ldappc.xsd">

  <targets id="ActiveDirectory">

    <target id="ad-prod" provider="provider-ad-prod">

      <object id="group" authoritative="true" targetId="ad-prod">
        <identifier ref="groupDn-ad-prod" baseId="ou=groups,dc=grouper,dc=edu" />
        <attribute name="objectclass" ref="ad-objectclass" />
        <attribute name="cn" />
        <attribute name="description" />
        <attribute name="hasMember" />
        <references name="member">
          <reference ref="members-jdbc" toObject="member" />
          <reference ref="members-g:gsa" toObject="group" />
        </references>
      </object>

      <object id="member" targetId="ad-prod">
        <identifier ref="memberDn-ad-prod" baseId="ou=people,dc=grouper,dc=edu" />
        <attribute name="isMemberOf" ref="memberIsMemberOf" />
      </object>

    </target>

    <target id="ad-test" provider="provider-ad-test">

      <object id="group" authoritative="true" targetId="ad-test">
        <identifier ref="groupDn-ad-test" baseId="ou=groups,dc=grouper,dc=edu" />
        <attribute name="objectclass" ref="ad-objectclass" />
        <attribute name="cn" />
        <attribute name="description" />
        <attribute name="hasMember" />
        <references name="member">
          <reference ref="members-jdbc" toObject="member" />
          <reference ref="members-g:gsa" toObject="group" />
        </references>
      </object>

      <object id="member" targetId="ad-test">
        <identifier ref="memberDn-ad-test" baseId="ou=people,dc=grouper,dc=edu" />
        <attribute name="isMemberOf" ref="memberIsMemberOf" />
      </object>

    </target>

  </targets>

  <targets id="OpenLDAP">

    <target id="openldap-prod" provider="provider-openldap-prod">


      <object id="group" authoritative="true" targetId="openldap-prod">
        <identifier ref="groupDn-openldap-prod" baseId="ou=groups,dc=grouper,dc=edu" />
        <attribute name="objectclass" ref="openldap-objectclass" />
        <attribute name="cn" />
        <attribute name="description" />
        <attribute name="hasMember" ref="hasMember" />
        <references name="member">
          <reference ref="members-jdbc" toObject="member" />
          <reference ref="members-g:gsa" toObject="group" />
        </references>
      </object>

      <object id="member" targetId="openldap-prod">
        <identifier ref="memberDn-openldap-prod" baseId="ou=people,dc=grouper,dc=edu" />
        <attribute name="isMemberOf" ref="memberIsMemberOf" />
        <references name="memberOf">
          <reference ref="memberIsMemberOf" toObject="group" />
        </references>
      </object>

    </target>

    <target id="openldap-test" provider="provider-openldap-test">

      <object id="group" authoritative="true" targetId="openldap-test">
        <identifier ref="groupDn-openldap-test" baseId="ou=groups,dc=grouper,dc=edu" />
        <attribute name="objectclass" ref="openldap-objectclass" />
        <attribute name="cn" />
        <attribute name="description" />
        <attribute name="hasMember" ref="hasMember" />
        <references name="member">
          <reference ref="members-jdbc" toObject="member" />
          <reference ref="members-g:gsa" toObject="group" />
        </references>
      </object>

      <object id="member" targetId="openldap-test">
        <identifier ref="memberDn-openldap-test" baseId="ou=people,dc=grouper,dc=edu" />
        <attribute name="isMemberOf" ref="memberIsMemberOf" />
        <references name="memberOf">
          <reference ref="memberIsMemberOf" toObject="group" />
        </references>
      </object>

    </target>

  </targets>

</ldappc>