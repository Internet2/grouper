<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="edu.internet2.middleware.grouper">

  <class name="GrouperAttribute" table="grouper_attribute" mutable="true">
    <composite-id access="field">
      <key-property name="groupKey"   type="string" column="groupKey"/>
      <key-property name="groupField" type="string" column="groupField"/>
    </composite-id>
    <property name="groupFieldValue">
      <column name="groupFieldValue" length="255"/>
    </property>
  </class>

  <class name="GrouperField" table="grouper_field" mutable="false">
    <cache usage="read-only"/>
    <id name="groupField" type="string" unsaved-value="null" >
      <column name="groupField" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="readPriv">
      <column name="readPriv" length="255" not-null="true"/>
    </property>
    <property name="writePriv">
      <column name="writePriv" length="255" not-null="true"/>
    </property>
    <property name="isList">
      <column name="isList" length="255" not-null="true"/>
    </property>
  </class>

  <class name="GrouperGroup" table="grouper_group" mutable="true">
    <id name="groupKey" type="string">
      <column name="groupKey" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="groupID">
      <column name="groupID" length="255" not-null="true"/>
    </property>
    <property name="createTime">
      <column name="createTime" length="255"/>
    </property>
    <property name="createSubject">
      <column name="createSubject" length="255"/>
    </property>
    <property name="createSource">
      <column name="createSource" length="255"/>
    </property>
    <property name="modifyTime">
      <column name="modifyTime" length="255"/>
    </property>
    <property name="modifySubject">
      <column name="modifySubject" length="255"/>
    </property>
    <property name="modifySource">
      <column name="modifySource" length="255"/>
    </property>
    <property name="comment">
      <column name="comment" length="255"/>
    </property>
  </class>

  <class name="GrouperList" table="grouper_list" mutable="true">
    <composite-id access="field">
      <key-property name="groupKey"   type="string" column="groupKey"/>
      <key-property name="groupField" type="string" column="groupField"/>
      <key-property name="memberKey"  type="string" column="memberKey"/>
      <key-property name="via"        type="string" column="via"/>
    </composite-id>
    <property name="removeAfter">
      <column name="removeAfter" length="255"/>
    </property>
  </class>

  <class name="GrouperMember" table="grouper_member">
    <id name="memberKey" type="string" unsaved-value="null">
      <column name="memberKey" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="memberID">
      <column name="memberID" length="255" not-null="true"/>
    </property>
    <property name="subjectID">
      <column name="subjectID" sql-type="char(255)"/>
    </property>
    <property name="subjectTypeID">
      <column name="subjectTypeID" sql-type="char(255)"/>
    </property>
  </class>

  <class name="GrouperSchema" table="grouper_schema" mutable="true">
    <composite-id access="field">
      <key-property name="groupKey"   type="string" column="groupKey"/>
      <key-property name="groupType"  type="string" column="groupType"/>
    </composite-id>
  </class>

  <class name="GrouperSession" table="grouper_session">
    <id name="sessionID" type="string" unsaved-value="null" >
      <column name="sessionID" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="subjectID">
      <column name="subjectID" length="255" not-null="true"/>
    </property>
    <property name="startTime">
      <column name="startTime" length="255" not-null="true"/>
    </property>
  </class>

  <class name="GrouperSubjectAttribute" table="grouper_SubjectAttribute">
    <composite-id access="field">
      <key-property name="subjectID"     type="string" column="subjectID"/>
      <key-property name="subjectTypeID" type="string" column="subjectTypeID"/>
    </composite-id> 
    <property name="name">
      <column name="name" sql-type="char(255)" not-null="true"/>
    </property>
    <property name="instance">
      <column name="instance" sql-type="integer"/>
    </property>
    <property name="value">
      <column name="value" sql-type="char(255)" not-null="true"/>
    </property>
    <property name="searchValue">
      <column name="searchValue" sql-type="char(255)" not-null="true"/>
    </property>
  </class>

  <class name="GrouperType" table="grouper_type" mutable="false">
    <cache usage="read-only"/>
    <id name="groupType" type="string" unsaved-value="null" >
      <column name="groupType" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
  </class>

  <class name="GrouperTypeDef" table="grouper_typeDef" mutable="false">
    <cache usage="read-only"/>
    <composite-id access="field">
      <key-property name="groupType"  type="string" column="groupType"/>
      <key-property name="groupField" type="string"  column="groupField"/>
    </composite-id>
  </class>

  <class name="SubjectImpl" table="grouper_subject">
    <id name="subjectID" type="string" unsaved-value="null" >
      <column name="subjectID" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="subjectTypeID">
      <column name="subjectTypeID" sql-type="char(255)" not-null="true"/>
    </property>
  </class>

  <class name="SubjectTypeImpl" table="grouper_subjectType" mutable="false">
    <cache usage="read-only"/>
    <id name="subjectTypeID" type="string" unsaved-value="null">
      <column name="subjectTypeID" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="name">
      <column name="name" sql-type="char(255)" not-null="true"/>
    </property>
    <property name="adapterClass">
      <column name="adapterClass" sql-type="char(255)" not-null="true"/>
    </property>
  </class>

</hibernate-mapping>

