<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="edu.internet2.middleware.directory.grouper">

  <class name="GrouperField" table="grouper_fields">
    <id name="groupField" type="string" unsaved-value="null" >
      <column name="groupField" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="readPriv">
      <column name="readPriv" length="255" not-null="true"/>
    </property>
    <property name="writePriv">
      <column name="writePriv" length="255" not-null="true"/>
    </property>
    <property name="isList">
      <column name="isList" length="255" not-null="true"/>
    </property>
  </class>

  <class name="GrouperGroupType" table="grouper_groupType">
    <id name="groupType" type="integer" unsaved-value="null" >
      <column name="groupType" sql-type="integer" not-null="true"/>
      <generator class="assigned"/>
    </id>
  </class>

  <class name="GrouperSession" table="grouper_session">
    <id name="sessionID" type="string" unsaved-value="null" >
      <column name="sessionID" sql-type="char(255)" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="cred">
      <column name="cred" length="255" not-null="true"/>
    </property>
    <property name="startTime">
      <column name="startTime" length="255" not-null="true"/>
    </property>
  </class>

</hibernate-mapping>

